{
  "name": "typescript-create-react-app",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "axios": "0.18.0",
    "connected-react-router": "4.5.0",
    "history": "4.7.2",
    "react": "16.6.0",
    "react-dom": "16.6.0",
    "react-hot-loader": "4.3.11",
    "react-loadable": "5.5.0",
    "react-redux": "5.1.0",
    "react-router-dom": "4.3.1",
    "redux": "4.0.1",
    "redux-devtools-extension": "2.13.5",
    "redux-saga": "0.16.2",
    "reset-css": "4.0.1",
    "uuid": "3.3.2"
  },
  "scripts": {
    "clean": "rimraf 'build/**/*'",
    "start": "npm run clean && cross-env NODE_ENV=staging react-app-rewired start --scripts-version react-scripts-ts",
    "staging": "npm run clean && cross-env NODE_ENV=staging react-app-rewired build --scripts-version react-scripts-ts",
    "production": "npm run clean && cross-env NODE_ENV=production react-app-rewired build --scripts-version react-scripts-ts",
    "test": "react-app-rewired test --scripts-version react-scripts-ts --env=jsdom",
    "eject": "react-scripts-ts eject",
    "analyze": "npm run build && source-map-explorer build/static/js/main.*"
  },
  "devDependencies": {
    "@types/axios": "0.14.0",
    "@types/jest": "23.3.7",
    "@types/node": "10.12.0",
    "@types/react": "16.4.18",
    "@types/react-dom": "16.0.9",
    "@types/react-loadable": "5.4.1",
    "@types/react-redux": "6.0.9",
    "@types/react-router-dom": "4.3.1",
    "@types/uuid": "3.4.4",
    "@types/webpack": "4.4.17",
    "@types/webpack-env": "1.13.6",
    "cross-env": "5.2.0",
    "node-sass": "4.9.4",
    "react-app-rewire-css-modules": "codebandits/react-app-rewire-css-modules",
    "react-app-rewired": "1.6.2",
    "react-scripts-ts": "3.1.0",
    "rimraf": "2.6.2",
    "sass-loader": "7.1.0",
    "source-map-explorer": "1.6.0",
    "typescript": "3.1.3"
  }
}
